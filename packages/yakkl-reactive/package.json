{
  "name": "@yakkl/reactive",
  "version": "0.1.0",
  "description": "YAKKL Reactive - High-performance reactive state management and data flow library",
  "license": "MIT",
  "author": "YAKKL Team",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules .turbo",
    "prepublishOnly": "pnpm run build"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./store": {
      "import": "./dist/store.mjs",
      "require": "./dist/store.js",
      "types": "./dist/store/index.d.ts"
    },
    "./computed": {
      "import": "./dist/computed.mjs",
      "require": "./dist/computed.js",
      "types": "./dist/computed/index.d.ts"
    },
    "./effect": {
      "import": "./dist/effect.mjs",
      "require": "./dist/effect.js",
      "types": "./dist/effect/index.d.ts"
    },
    "./operators": {
      "import": "./dist/operators.mjs",
      "require": "./dist/operators.js",
      "types": "./dist/operators/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {},
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "reactive",
    "state",
    "store",
    "browser-extension",
    "observable",
    "computed",
    "effect",
    "signal",
    "reactivity",
    "state-management",
    "data-flow",
    "yakkl"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yakkl/yakkl.git",
    "directory": "packages/yakkl-reactive"
  },
  "bugs": {
    "url": "https://github.com/yakkl/yakkl/issues"
  },
  "homepage": "https://github.com/yakkl/yakkl/tree/main/packages/yakkl-reactive#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
