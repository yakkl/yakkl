{
  "name": "@yakkl/core",
  "version": "0.1.0",
  "description": "YAKKL Core Wallet Engine - Foundation for all YAKKL products",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "vitest",
    "lint": "eslint src",
    "format": "prettier --write src"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./mods": {
      "import": "./dist/mods.mjs",
      "require": "./dist/mods.js",
      "types": "./dist/mods/index.d.ts"
    },
    "./embedded": {
      "import": "./dist/embedded.mjs",
      "require": "./dist/embedded.js",
      "types": "./dist/embedded/index.d.ts"
    }
  },
  "dependencies": {
    "decimal.js": "^10.6.0",
    "ethers": "^5.7.2",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/decimal.js": "^7.4.3",
    "@types/estree": "^1.0.8",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "wallet",
    "crypto",
    "blockchain",
    "ethereum",
    "web3",
    "embedded",
    "mods",
    "plugins"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yakkl/yakkl.git",
    "directory": "packages/yakkl-core"
  }
}