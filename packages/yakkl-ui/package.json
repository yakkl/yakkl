{
  "name": "@yakkl/ui",
  "version": "0.1.0",
  "description": "Shared UI components for YAKKL ecosystem",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./src/components/Button.svelte": {
      "svelte": "./src/components/Button.svelte",
      "default": "./src/components/Button.svelte"
    },
    "./src/components/Modal.svelte": {
      "svelte": "./src/components/Modal.svelte",
      "default": "./src/components/Modal.svelte"
    },
    "./src/components/Avatar.svelte": {
      "svelte": "./src/components/Avatar.svelte",
      "default": "./src/components/Avatar.svelte"
    },
    "./src/components/Card.svelte": {
      "svelte": "./src/components/Card.svelte",
      "default": "./src/components/Card.svelte"
    },
    "./src/components/AddressDisplay.svelte": {
      "svelte": "./src/components/AddressDisplay.svelte",
      "default": "./src/components/AddressDisplay.svelte"
    },
    "./src/components/TokenBalance.svelte": {
      "svelte": "./src/components/TokenBalance.svelte",
      "default": "./src/components/TokenBalance.svelte"
    },
    "./src/components/ErrorDialog.svelte": {
      "svelte": "./src/components/ErrorDialog.svelte",
      "default": "./src/components/ErrorDialog.svelte"
    },
    "./src/components/Loading.svelte": {
      "svelte": "./src/components/Loading.svelte",
      "default": "./src/components/Loading.svelte"
    },
    "./src/components/LoadingSpinner.svelte": {
      "svelte": "./src/components/LoadingSpinner.svelte",
      "default": "./src/components/LoadingSpinner.svelte"
    },
    "./src/components/Input.svelte": {
      "svelte": "./src/components/Input.svelte",
      "default": "./src/components/Input.svelte"
    },
    "./src/components/Toast.svelte": {
      "svelte": "./src/components/Toast.svelte",
      "default": "./src/components/Toast.svelte"
    },
    "./src/components/FailureDialog.svelte": {
      "svelte": "./src/components/FailureDialog.svelte",
      "default": "./src/components/FailureDialog.svelte"
    },
    "./src/components/ErrorDialog.svelte": {
      "svelte": "./src/components/ErrorDialog.svelte",
      "default": "./src/components/ErrorDialog.svelte"
    },
    "./src/components/Tooltip.svelte": {
      "svelte": "./src/components/Tooltip.svelte",
      "default": "./src/components/Tooltip.svelte"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "svelte": "./dist/components/*.js",
      "import": "./dist/components/*.js"
    },
    "./styles": {
      "import": "./dist/styles/index.css"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@yakkl/core": "workspace:*",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@types/node": "^20.11.24",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.6.0",
    "postcss": "^8.4.35",
    "rimraf": "^5.0.5",
    "svelte": "^5.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "keywords": [
    "yakkl",
    "ui",
    "components",
    "svelte",
    "tailwind",
    "daisyui"
  ],
  "author": "YAKKL",
  "license": "MIT"
}
