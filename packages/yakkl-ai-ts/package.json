{
  "name": "@yakkl/ai-ts",
  "version": "1.0.0",
  "description": "YAKKL AI Library - TypeScript implementation for multi-provider AI integration with RAG support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "doc": "typedoc --out docs/api src/index.ts",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "ai",
    "llm",
    "rag",
    "openai",
    "anthropic",
    "google",
    "gemini",
    "vectorstore",
    "embeddings",
    "yakkl"
  ],
  "author": "YAKKL",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "p-queue": "^7.4.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "openai": "^4.20.0",
    "@anthropic-ai/sdk": "^0.10.0",
    "@google-ai/generativelanguage": "^1.0.0",
    "cohere-ai": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "@google-ai/generativelanguage": {
      "optional": true
    },
    "cohere-ai": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yakkl/yakkl-ai-ts.git"
  },
  "bugs": {
    "url": "https://github.com/yakkl/yakkl-ai-ts/issues"
  },
  "homepage": "https://github.com/yakkl/yakkl-ai-ts#readme"
}