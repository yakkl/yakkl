{
  "name": "@yakkl/cache",
  "version": "0.1.0",
  "description": "Unified caching system for YAKKL ecosystem - blockchain, semantic, and transaction caching",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./strategies": {
      "types": "./dist/strategies/index.d.ts",
      "import": "./dist/strategies/index.mjs",
      "require": "./dist/strategies/index.js"
    },
    "./tiers": {
      "types": "./dist/tiers/index.d.ts",
      "import": "./dist/tiers/index.mjs",
      "require": "./dist/tiers/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf dist",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@yakkl/core": "workspace:*",
    "idb": "^8.0.0",
    "lru-cache": "^10.0.0",
    "p-queue": "^8.0.0",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "yakkl",
    "cache",
    "blockchain",
    "semantic",
    "performance",
    "browser-extension"
  ],
  "author": "YAKKL",
  "license": "MIT"
}